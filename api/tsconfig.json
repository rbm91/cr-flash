{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "../.vercel/output/functions",
    "rootDir": ".",
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./**/*.ts"]
}
